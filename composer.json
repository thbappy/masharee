{

    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-dom": "*",
        "andreaselia/analytics": "^1.13",
        "artesaos/seotools": "^0.22.0",
        "automattic/woocommerce": "^3.1",
        "barryvdh/laravel-dompdf": "^1.0.2",
        "bumbummen99/shoppingcart": "^4.2",
        "doctrine/dbal": "^3.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "kkomelin/laravel-translatable-string-exporter": "^1.11",
        "ladumor/laravel-pwa": "^0.0.4",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.16",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laraveldaily/laravel-invoices": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.16",
        "mews/purifier": "^3.3",
        "milon/barcode": "^10.0",
        "nwidart/laravel-modules": "^8.3",
        "pion/laravel-chunk-upload": "^1.5",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-activitylog": "^4.5.3",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-translatable": "^6.0.0",
        "spatie/laravel-webhook-server": "^3.4",
        "stancl/tenancy": "^3.4",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "symfony/postmark-mailer": "^6.0",
        "twilio/sdk": "^7.11",
        "xgenious/xgapiclient": "^1.2",
        "yajra/laravel-datatables-oracle": "~9.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Plugins\\": "plugins/",
            "Modules\\": "Modules/"

        },
        "files": [
            "app/Helpers/funtions.php",
            "app/Helpers/module-helper.php",
            "app/Helpers/theme-helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
